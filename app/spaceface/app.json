{
  "expo": {
    "name": "spaceface",
    "slug": "spaceface",
    "platforms": ["ios", "android", "web"],
    "version": "1.0.3",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "android": {
      "package": "com.spacefacehq.spaceface",
      "adaptiveIcon": {
        "foregroundImage": "assets/images/icons/TrumpSaidWhat-foreground-2.png",
        "backgroundColor": "#eb5d48"
      },
      "icon": "assets/images/icons/TrumpSaidWhat-old-icon.png",
      "versionCode": 4
    }
  }
}
